{
  "name": "biolab-ai",
  "version": "1.0.0",
  "description": "An AI-powered laboratory management and analysis system using Google Gemini API",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
